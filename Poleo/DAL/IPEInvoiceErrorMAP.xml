<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="LineItem"
xmlns="http://ibatis.apache.org/mapping"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <alias>
    <typeAlias alias="IPEInvoiceError" type="Poleo.Objects.IPEInvoiceError" />
  </alias>

  <statements>
    <insert id="InsertIPEInvoiceError" parameterClass="IPEInvoiceError">
      INSERT INTO SM_IPEInvoiceError (Numer_Tienda, Number_Invoice, Date_Invoice)
      VALUES (#Numer_Tienda#, #Number_Invoice#, #Date_Invoice#)
    </insert>
  </statements>
</sqlMap>
